{
    "manifest_version": 3,
    "name": "Unnamed extension",
    "version": "1.0",
    "description": "Ai assistant for Remix IDE",
    "action": {
      "default_title": "Unnamed AI",
      "default_popup": "popup.html"

    },
    "content_scripts":[
        {
        "js":["main.js"],
        "matches":[
            "https://remix.ethereum.org/*"
        ]
        }
    ],
    "icons": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  }